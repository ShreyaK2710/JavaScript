//assign your age to var
const myAge=19;
//assign 2 to var earlyYears
let earlyYears=2;
earlyYears*=10.5;
//set result equal to laterYears
let laterYears=myAge-2;
laterYears*=4;
console.log(earlyYears);
console.log(laterYears);
//add the vars and store it
myAgeInDogYears=earlyYears+laterYears;
myName='Trixi';
'Trixi'.toLowerCase()
//use string interpolation for result
console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`);
